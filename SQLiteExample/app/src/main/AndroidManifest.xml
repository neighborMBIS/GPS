<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.computer.sqliteexample" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".View.BeforeLogin.MainActivity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".View.BeforeLogin.MembershipActivity"
            android:label="@string/title_activity_main">
        </activity>
        <activity
            android:name=".View.AfterLogin.SuccessLoginActivity"
            android:label="@string/title_activity_success_login" >
        </activity>
        <activity
            android:name=".View.AfterLogin.ListActivity"
            android:label="@string/title_activity_list" >
        </activity>
        <activity
            android:name=".View.AfterLogin.MyInfoActivity"
            android:label="@string/title_activity_my_info" >
        </activity>
    </application>

</manifest>
